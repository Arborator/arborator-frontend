{
  "name": "Arborator-Grew",
  "version": "1.0.0",
  "description": "Quasar Vue Frontend for Arborator-Grew",
  "productName": "Arborator-Grew",
  "cordovaId": "ilpga.arborator",
  "author": "gguibon <gael.guibon@inria.fr>",
  "private": true,
  "scripts": {
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.8.2",
    "autoprefixer": "^9.6.2",
    "axios": "^0.18.1",
    "quasar": "^1.12.8",
    "trim-right": "^1.0.1",
    "vue-codemirror": "^4.0.6",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.0.0",
    "vue-ls": "^3.2.1",
    "vue-select": "^3.2.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.9.6",
    "@quasar/quasar-app-extension-qmediaplayer": "^1.0.27",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.13",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.1.0-beta.2",
    "cypress-social-logins": "^1.3.2",
    "eslint-plugin-cypress": "^2.10.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
