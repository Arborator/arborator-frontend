{
  "name": "Arborator-Grew",
  "version": "1.0.0",
  "description": "Quasar Vue Frontend for Arborator-Grew",
  "productName": "Arborator-Grew",
  "cordovaId": "ilpga.arborator",
  "author": "gguibon <gael.guibon@inria.fr>",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/quasar dev",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "buildProd": "mv dist/ dist_backup/ ; quasar build && rm -r dist_backup/ || mv dist_backup/ dist/",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "node_modules/.bin/quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"node_modules/.bin/quasar dev\" \"jest --watch\"",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.11.3",
    "autoprefixer": "^10.3.7",
    "axios": "^0.24.0",
    "codemirror": "^5.63.3",
    "codemirror-editor-vue3": "^0.2.4",
    "conllup": "^1.1.6",
    "dependencytreejs": "^1.0.5",
    "diff": "^5.0.0",
    "install": "^0.13.0",
    "jszip": "^3.7.1",
    "npm": "^8.1.1",
    "postcss": "^8.3.11",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.2.0",
    "tailwindcss": "^2.2.17",
    "trim-right": "^1.0.1",
    "typescript": "^4.4.4",
    "vue-audio-visual": "^2.3.0",
    "vue-i18n": "^9.2.0-beta.15",
    "vue-ls": "^3.2.2",
    "vue-router": "^4.0.12",
    "vue-select": "^3.16.0",
    "vue3-cookies": "^1.0.1",
    "vue3-storage": "^0.1.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@quasar/app": "^3.1.7",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.13",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.1.0-beta.7",
    "axios-mock-adapter": "^1.19.0",
    "cypress-social-logins": "^1.3.2",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.2",
    "jest-mock-axios": "^4.2.1",
    "quasar": "^2.2.1",
    "vue-loader": "^15.9.8"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
